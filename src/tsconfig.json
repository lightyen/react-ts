{
  "extends": "../tsconfig",
  "compilerOptions": {
    "noImplicitAny": false,
    "strictNullChecks": false, // Not worth when I use react ref, so close it.
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "lib": ["esnext", "dom", "dom.iterable", "webworker"],
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "assets/*": ["./assets/*"]
    }
  }
}
